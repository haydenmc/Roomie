
@{
	Layout = null;
}

<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<meta name="description" content="Roomie: A beautiful, fluid web-app that makes communication with your room mates easy." />
	<meta name="application-name" content="roomie" />
	<meta name="msapplication-TileColor" content="#000000" />
	<meta name="msapplication-square70x70logo" content="tiny.png" />
	<meta name="msapplication-square150x150logo" content="square.png" />
	<meta name="msapplication-wide310x150logo" content="wide.png" />
	<meta name="msapplication-square310x310logo" content="large.png" />
	<title>Roomie</title>
	@Styles.Render("~/bundle/Content/css")
</head>
<body>
	<noscript><h1>Roomie requires javascript.</h1></noscript>
	@Scripts.Render("~/bundle/Scripts/js")
	@Scripts.Render("~/bundle/Scripts/ts")
	<!-- SignalR -->
	<script src="~/signalr/hubs"></script>
</body>
</html>
